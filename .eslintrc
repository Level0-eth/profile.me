{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "no-unused-vars": "warn",
    "no-debugger": "error",
    "no-dupe-args": "error",
    "no-func-assign": "error",
    "no-new-symbol": "error"
  }
}
